-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local UIGradient = Instance.new("UIGradient")
local Credits = Instance.new("TextLabel")
local Equip = Instance.new("TextButton")
local Farm = Instance.new("TextButton")
local Gained = Instance.new("TextButton")
local Count = Instance.new("TextButton")
local Hud = Instance.new("TextButton")
local Ping = Instance.new("TextButton")
local Rumble = Instance.new("TextButton")
local Afk = Instance.new("TextButton")
local Lock = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.678274035, 0, 0.533447683, 0)
Frame.Size = UDim2.new(0, 404, 0, 229)
Frame.Active = true
Frame.Draggable = true

UICorner.Parent = Frame

UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 153, 10)), ColorSequenceKeypoint.new(0.91, Color3.fromRGB(255, 215, 160)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
UIGradient.Parent = Frame

Credits.Name = "Credits"
Credits.Parent = Frame
Credits.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Credits.BackgroundTransparency = 1.000
Credits.Size = UDim2.new(0, 404, 0, 50)
Credits.Font = Enum.Font.GothamBlack
Credits.Text = "Classic Click Lift - By Vert"
Credits.TextColor3 = Color3.fromRGB(0, 0, 0)
Credits.TextSize = 30.000

Equip.Name = "Equip"
Equip.Parent = Frame
Equip.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Equip.BackgroundTransparency = 1.000
Equip.Position = UDim2.new(0.62376231, 0, 0.781659365, 0)
Equip.Size = UDim2.new(0, 152, 0, 50)
Equip.Font = Enum.Font.GothamBlack
Equip.Text = "Equip Weights"
Equip.TextColor3 = Color3.fromRGB(0, 0, 0)
Equip.TextScaled = true
Equip.TextSize = 20.000
Equip.TextWrapped = true
Equip.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v.Name == "Double Weight" then
			v:Activate()
			v.Parent = game.Players.LocalPlayer.Character
		end
	end
end)

Farm.Name = "Farm"
Farm.Parent = Frame
Farm.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Farm.BackgroundTransparency = 1.000
Farm.Position = UDim2.new(0.599009871, 0, 0.388646305, 0)
Farm.Size = UDim2.new(0, 152, 0, 50)
Farm.Font = Enum.Font.GothamBlack
Farm.Text = "Click Lift"
Farm.TextColor3 = Color3.fromRGB(0, 0, 0)
Farm.TextScaled = true
Farm.TextSize = 20.000
Farm.TextWrapped = true
Farm.MouseButton1Click:connect(function()
	_G.Auto4 = true
	while _G.Auto4 do
		wait(4.2)
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v.Name == "Double Weight" then v:Activate() end


		end
	end
end)


