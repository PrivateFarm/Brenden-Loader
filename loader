Afk.Name = "Afk"
Afk.Parent = Frame
Afk.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Afk.BackgroundTransparency = 1.000
Afk.Position = UDim2.new(0.438118815, 0, 0.781659365, 0)
Afk.Size = UDim2.new(0, 65, 0, 50)
Afk.Font = Enum.Font.GothamBlack
Afk.Text = "AFK"
Afk.TextColor3 = Color3.fromRGB(0, 0, 0)
Afk.TextScaled = true
Afk.TextSize = 14.000
Afk.TextWrapped = true
Afk.MouseButton1Click:Connect(function()
	local VirtualUser=game:service'VirtualUser'
	game:service'Players'.LocalPlayer.Idled:connect(function()
		VirtualUser:CaptureController()
		VirtualUser:ClickButton2(Vector2.new())
	end)
end)

Lock.Name = "Lock"
Lock.Parent = Frame
Lock.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Lock.BackgroundTransparency = 1.000
Lock.Position = UDim2.new(0.507425725, 0, 0.174672484, 0)
Lock.Size = UDim2.new(0, 175, 0, 49)
Lock.Font = Enum.Font.GothamBlack
Lock.Text = "Lock PLR"
Lock.TextColor3 = Color3.fromRGB(0, 0, 0)
Lock.TextScaled = true
Lock.TextSize = 14.000
Lock.TextWrapped = true
Lock.MouseButton1Click:Connect(function()
	if Locked == false then
		Locked = true
		Lock(true)
	elseif Locked == true then
		Locked = false
		Lock(false)
	end
end)
